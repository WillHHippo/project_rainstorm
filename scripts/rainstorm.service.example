[Unit]
Description=Backend for the rainstorm project
After=network.target

[Service]
User=memoya
WorkingDirectory=/home/memoya/project_rainstorm/raincloud
ExecStart=/home/memoya/project_rainstorm/venv/bin/gunicorn -b 127.0.0.1:5000 raincloud:create_app
Restart=always

[Install]
WantedBy=multi-user.target
